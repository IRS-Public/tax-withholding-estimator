    <Fact path="/jobs/*/isPayFrequencyMonthly">
      <TaxYear>2025</TaxYear>
      <Derived>
        <Equal>
          <Left>
            <Dependency path="../payFrequency" />
          </Left>
          <Right>
            <Enum optionsPath="/payFrequencyOptions">monthly</Enum>
          </Right>
        </Equal>
      </Derived>
