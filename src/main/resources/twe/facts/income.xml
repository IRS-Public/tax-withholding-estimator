    <Fact path="/jobs/*/isPayFrequencyTwiceMonthly">
      <TaxYear>2025</TaxYear>
      <Derived>
        <Equal>
          <Left>
            <Dependency path="../payFrequency" />
          </Left>
          <Right>
            <Enum optionsPath="/payFrequencyOptions">twiceMonthly</Enum>
          </Right>
        </Equal>
      </Derived>
