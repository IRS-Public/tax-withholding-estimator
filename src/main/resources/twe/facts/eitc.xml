            </When>
            <Then>
              <Dependency path="/phaseInRatesFor1Qc"/>
            </Then>
          </Case>
          <Case>
            <When>
              <Equal>
                <Left>
                  <Dependency path="/eitcQualifyingChildren"/>
                </Left>
                <Right>
                  <Int>2</Int>
                </Right>
              </Equal>
