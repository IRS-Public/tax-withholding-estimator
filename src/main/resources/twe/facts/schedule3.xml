    <Fact path="/schedule3Line6g">
      <Description>Mortgage interest credit. Form 8396</Description>
      <Derived>
        <LesserOf>
          <Dependency path="/tentativeSchedule3Line6g"/>
          <Dependency path="/maxSchedule3Line6g"/>
        </LesserOf>
      </Derived>
    </Fact>
    <Fact path="/isSchedule3Line6gOverMax">
      <Derived>
        <Switch>
          <Case>
            <When>
              <GreaterThan>
                <Left>
                  <Dependency path="/tentativeSchedule3Line6g"/>
                </Left>
                <Right>
                  <Dependency path="/maxSchedule3Line6g"/>
                </Right>
              </GreaterThan>
            </When>
            <Then>
              <True/>
            </Then>
          </Case>
          <Case>
            <When>
              <True/>
            </When>
            <Then>
              <False/>
            </Then>
          </Case>
        </Switch>
      </Derived>
    </Fact>
