<?xml version="1.0"?>
<?xml-model href="./FactDictionaryModule.rng"?>
<FactDictionaryModule>
  <Facts>
    <Fact path="/hsaTotalDeductibleAmount">
      <Description>HSA total deductible amount, no logic in place for actually calculating the
        applicable
        amount</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/hsaTotalDeductibleAmountMax">
      <Description>Maximum annual deductible and other out-of-pocket expenses for family</Description>
      <TaxYear>2025</TaxYear>
      <Derived>
        <Dollar>16600</Dollar>
      </Derived>
    </Fact>
    <Fact path="/actualHsaTotalDeductibleAmount">
      <Description>HSA total deductible amount, no logic in place for actually calculating the
        applicable
        amount</Description>
      <Derived>
        <LesserOf>
          <Dependency path="/hsaTotalDeductibleAmount"/>
          <Dependency path="/hsaTotalDeductibleAmountMax"/>
        </LesserOf>
      </Derived>
    </Fact>
    <Fact path="/deductionForTraditionalIRAContribution">
      <Description>Deduction for contributions to traditional IRAs (excluding payroll deductions)</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/deductionForTraditionalIRAContributionMax">
      <Description>Max contribution for traditional IRAs (excluding payroll deductions) is $8000 per year. $7000 is the limit for those under 50, but we only know if someone is under 65 or not
      We are agnostic to filing status so double to 16000 and do not take MAGI into account for simplified computation
      </Description>
      <Derived>
        <Dollar>16000</Dollar>
      </Derived>
    </Fact>
    <Fact path="/actualDeductionForTraditionalIRAContribution">
      <Description>Deduction for contributions to traditional IRAs (excluding payroll deductions)</Description>
      <Derived>
        <LesserOf>
          <Dependency path="/deductionForTraditionalIRAContribution"/>
          <Dependency path="/deductionForTraditionalIRAContributionMax"/>
        </LesserOf>
      </Derived>
    </Fact>
    <Fact path="/actualMovingExpensesForArmedServicesMembers">
      <Description>Moving expenses for members of armed services</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/actualAlimonyPaid">
      <Description>Alimony paid (pre 2019 divorce)</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/actualPenaltyForEarlySavingsWithdrawal">
      <Description>Penalty for early withdrawal of savings</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/selfEmploymentHealthInsuranceContributions">
      <Description>Self-employed health insurance contributions. Implements Form 7206</Description>
      <Override>
        <Condition>
          <Not>
            <Dependency path="/eitherTPHasSelfEmploymentIncome"/>
          </Not>
        </Condition>
        <Default>
          <Dollar>0</Dollar>
        </Default>
      </Override>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/actualBusinessCreditsForEligible">
      <Description>Business credits for reservists, performing artists, and fee-based government
        officials</Description>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/selfEmployedRetirementPlanContributions">
      <Description>Self-employed contributions to eligible retirement plans.</Description>
      <Override>
        <Condition>
          <Not>
            <Dependency path="/eitherTPHasSelfEmploymentIncome"/>
          </Not>
        </Condition>
        <Default>
          <Dollar>0</Dollar>
        </Default>
      </Override>
      <Writable>
        <Dollar/>
      </Writable>
    </Fact>
    <Fact path="/adjustmentsToIncomeExcludingStudentLoanInterest">
      <Description>The adjustments to income used in the calculation of non-IRA deduction MAGIs to determine deducible
        amounts. Used in the Student Loan Interest Deduction and Social Security Benefits Worksheets. This should
        include be a sum of Schedule 1, lines 11 through 20, and 23 and 25</Description>
      <Derived>
        <Add>
          <Dependency path="/actualEducatorExpensesAdjustmentAmount"/>
          <Dependency path="/actualBusinessCreditsForEligible"/>
          <Dependency path="/actualHsaTotalDeductibleAmount"/>
          <Dependency path="/selfEmploymentTaxDeduction"/>
          <Dependency path="/actualMovingExpensesForArmedServicesMembers"/>
          <Dependency path="/actualSelfEmployedRetirementPlanDeduction"/>
          <Dependency path="/actualSelfEmploymentHealthInsuranceDeduction"/>
          <Dependency path="/actualPenaltyForEarlySavingsWithdrawal"/>
          <Dependency path="/actualAlimonyPaid"/>
          <Dependency path="/actualDeductionForTraditionalIRAContribution"/>
        </Add>
      </Derived>
    </Fact>
    <Fact path="/adjustmentsToIncome">
      <Description>Adjustments to income to report on line 10 of Form 1040 and line 26 of Schedule 1.</Description>
      <Derived>
        <GreaterOf>
          <Round>
            <Add>
              <Dependency path="/adjustmentsToIncomeExcludingStudentLoanInterest"/>
              <Dependency path="/actualStudentLoanInterestAdjustmentAmount"/>
            </Add>
          </Round>
          <Dollar>0</Dollar>
        </GreaterOf>
      </Derived>
    </Fact>
  </Facts>
</FactDictionaryModule>
