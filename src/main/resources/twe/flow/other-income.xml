<FlowConfigModule>
  <page title="Other Income" route="other-income">
    <section>
      <h1>Other Income</h1>
      
      <h2>Net Self-Employment Income</h2>
      <fg-set path="/netSelfEmploymentIncome">
        <question>How much was your net self-employment income this year?</question>
        <input type="dollar"/>
      </fg-set>
      
      <h2>Ordinary Dividends Income</h2>
      <fg-set path="/ordinaryDividendsIncome">
        <question>How much have you made in ordinary dividends income so far this year?</question>
        <input type="dollar"/>
      </fg-set>
      
      <h2>Unemployment Insurance Income</h2>
      <fg-set path="/unemploymentInsuranceIncome">
        <question>How much have you received in employment insurance income so far this year?</question>
        <input type="dollar"/>
      </fg-set>
      
      <h2>Miscellaneous Income</h2>
      <fg-set path="/miscIncome">
        <question>How much other income have you made this year?</question>
        <input type="dollar"/>
      </fg-set>
    </section>
  </page>
</FlowConfigModule>
